{
    "name": "accelor-web",
    "version": "1.0.0",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "alias": {
        "resolve": {
            "react": "preact/compat",
            "react-dom": "preact/compat"
        }
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development next",
        "build": "next build",
        "build:ssg": "cross-env NODE_ENV=production next build && next export -o build",
        "build:ssr": "cross-env NODE_ENV=production next build",
        "start": "cross-env NODE_ENV=production next start"
    },
    "devDependencies": {
        "@babel/plugin-transform-spread": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/node": "^17.0.17",
        "@types/react": "^17.0.39",
        "babel-plugin-inline-react-svg": "^2.0.1",
        "babel-preset-next": "^1.4.0",
        "cross-env": "^7.0.2",
        "style-loader": "^3.3.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@babel/core": "^7.17.10",
        "@babel/plugin-transform-react-jsx": "^7.17.3",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/plugin-transform-typescript": "^7.16.8",
        "@preact/compat": "^17.1.1",
        "@svgr/webpack": "^6.2.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "next": "^12.1.6",
        "next-plugin-preact": "^3.0.7",
        "node-sass": "^7.0.1",
        "path": "^0.12.7",
        "preact": "^10.8.1",
        "preact-render-to-string": "^5.2.0",
        "react": "npm:@preact/compat@^17.0.3",
        "react-dom": "npm:@preact/compat@^17.1.1",
        "react-ssr-prepass": "^1.5.0",
        "sass": "^1.49.9",
        "stylelint": "^14.5.3",
        "swr": "^1.3.0",
        "webpack-stream": "^7.0.0"
    },
    "license": "UNLICENSED"
}
